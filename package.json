{
  "name": "unvibe",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:client": "react-router build",
    "dev:client": "react-router dev",
    "start:client": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --noEmit",
    "dev": "concurrently \"next dev --port=3005\" \"npm run dev:server\"",
    "dev:server": "npm run server:nodemon",
    "build": "npm run build:client",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:server": "tsx --env-file=.env.local server/index.ts",
    "lint": "next lint",
    "server:nodemon": "nodemon --watch ./server --watch ./plugins --ext ts --exec \"npm run server\"",
    "server": "tsx --env-file=.env.local server/index.ts"
  },
