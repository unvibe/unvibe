  "scripts": {
    "server:nodemon": "nodemon --watch ./.env.local --watch ./server --watch ./plugins --watch ./environment --ext ts --exec \"npm run server\"",
    "server": "tsx --env-file=.env.local server/index.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npx drizzle-kit studio\"",
    "dev:server": "npm run server:nodemon",
    "dev:client": "react-router dev",
    "build:client": "react-router build",
    "build": "npm run build:client",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\" \"npx drizzle-kit studio\"",
    "start:server": "tsx --env-file=.env.local server/index.ts",
    "start:client": "PORT=54495 react-router-serve ./build/server/index.js",
    "lint": "next lint",
    "typecheck": "react-router typegen && tsc --noEmit"
